{
    "name": "latublu/php-database-test",
    "type": "project",
    "description": "a project to test PHP database connections for MySQL and ScyllaDB/Cassandra",
    "keywords": [
        "php",
        "database",
        "extensions",
        "mysql",
        "scylladb",
        "cassandra"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Lance Turner",
            "email": "me@latu.net",
            "homepage": "https://latu.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpstan/phpstan": "^1.10",
        "mockery/mockery": "^1.6"
    },
    "require": {
        "php": "^8.1",
        "ext-cassandra": "^1.3",
        "vlucas/phpdotenv": "^5.6",
        "hassankhan/config": "^3.1",
        "phpunit/phpunit": "^10.5"
    }
}
